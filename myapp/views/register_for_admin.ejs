<!doctype html>
<html lang="ko">

<head>
  <%-include('./layout/import_head')%>
  <link rel="stylesheet" href="./stylesheets/register_for_admin.css">
</head>

<body>
  <%-include('./layout/header')%>

    <div class="Container">
      <!-- login form -->
        <div class="login-form">
          <form id="submitForm" method="POST" action="/registerProcessForAdmin" onsubmit="return checkForm()">
            <%-include('./layout/register_form')%>
            <label class="form-label">소속 분류</label>
            <label class="form-text">선택 입니다.</label>
            <div id="form-check">
            </div>
            <button type="submit" class="btn btn-color-orange">가입하기</button>
          </form>
        </div>
    </div>

    <!-- Optional JavaScript; choose one of the two! -->

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
      crossorigin="anonymous"></script>
    <script src="./javascripts/register.js"></script>
    <script src="./javascripts/form_check.js"></script>
</body>

</html>